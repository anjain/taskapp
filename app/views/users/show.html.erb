<body class="bg-info">
  <%= render 'layouts/header' %>
  <div class="center" align="center">
  <h1 align="center">All Users</h1>
  <ol class="list-group list-group-flush" align="center">
    <% @users.each do |user| %>
      <li class="list-group-item"><%= link_to user.email , user_path(id: user.id) %></li>
    <%end%>
  </ol>
  </div>
</body>